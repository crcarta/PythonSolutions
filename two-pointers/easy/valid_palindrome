class Solution:
    def isPalindrome(self, s: str) -> bool:
        l = len(s)-1
        i = 0
        while(i < l):
            if not s[i].isalnum():
                i+=1
                continue
            if not s[l].isalnum():
                l-=1
                continue
            if s[i].lower() != s[l].lower():
                return False    
            i+=1
            l-=1
        
        return True

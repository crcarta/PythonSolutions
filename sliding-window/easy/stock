class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        if len(prices) < 2:
            return 0
        left=0
        maxprofit=0

        for right in range (1, len(prices)):
            if prices[right] < prices[left]:
                left=right
            else:
                currentprofit = prices[right]-prices[left]
                maxprofit = max(currentprofit, maxprofit)
        
        return maxprofit

#Bucket Sort
#Must be better than 0(nlog(n))

class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        vec = defaultdict(int)
        for i in nums:
            vec[i]+=1
        buckets = [[] for _ in range(len(nums)+1)]
        for n, c in vec.items():
            buckets[c].append(n)

        result=[]
        for c in range(len(nums), 0, -1):
            for n in buckets[c]:
                result.append(n)
                if len(result)==k:
                    return result
        

